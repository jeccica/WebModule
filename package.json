{
  "name":           "webmodule",
  "version":        "0.0.5",
  "description":    "The WebModule is module template for Mobile Web Application.",
  "url":            "https://github.com/uupaa/Zzz.js",
  "keywords":       ["Zzz", "util", "uupaa", "WebModule"],
  "repository": {
    "type":         "git",
    "url":          "https://github.com/uupaa/Zzz.js.git"
  },
  "scripts": {
    "watch":        "node node_modules/uupaa.watch.js --verbose --action build ./lib/Zzz.js",
    "build":        "node node_modules/uupaa.minify.js --output ./lib/Zzz.min.js --verbose --keep --pretty ./lib/Zzz.js",
    "build-simple": "node node_modules/uupaa.minify.js --output ./lib/Zzz.min.js --verbose --keep --pretty --nowrap --simple ./lib/Zzz.js",
    "build-release":"node node_modules/uupaa.minify.js --output ./lib/Zzz.min.js --verbose --build all",
    "test":         "NODE_ENV=production NODE_PATH=lib node --harmony test/index.node.js; open test/index.html",
    "lint":         "node lint/plato.js; open lint/plato/index.html",
    "alt":          "npm run tsc; npm run build",
    "alt-simple":   "npm run tsc; npm run build-simple",
    "tsc":          "tsc -m commonjs -d ./alt/Zzz.ts; cp ./alt/Zzz.js ./lib/Zzz.js"
  },
  "build": {
    "all": { "lib/Zzz.js": "" },
    "node": { },
    "worker": { },
    "browser": { }
  },
  "dependencies": {
  },
  "devDependencies": {
    "uupaa.valid.js":   "",
    "uupaa.help.js":    "",
    "uupaa.task.js":    "",
    "uupaa.test.js":    "",
    "uupaa.watch.js":   "",
    "uupaa.minify.js":  ""
  },
  "lib":            "./lib/",
  "main":           "./index.js",
  "author":         "uupaa <uupaa.js@gmail.com>",
  "license":        "MIT",
  "contributors":   []
}
